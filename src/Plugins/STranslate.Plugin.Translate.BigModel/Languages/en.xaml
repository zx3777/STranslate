<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <sys:String x:Key="STranslate_Plugin_Translate_BigModel_Url">Url</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_BigModel_Url_Description">Configure the BigModel API service address.&#x0a;If the Path is not filled in, it will be automatically populated with /api/paas/v4/chat/completions.&#x0a;For example, for the BigModel API address https://open.bigmodel.cn/api/paas/v4/chat/completions, the Path is /api/paas/v4/chat/completions.</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_BigModel_ApiKey">ApiKey</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_BigModel_ApiKey_Placeholder">Please enter your apiKey</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_BigModel_ApiKey_Description">Enter your BigModel API key</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_BigModel_Model">Select a model</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_BigModel_Prompt_Header">Prompt Configuration</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_BigModel_Prompt_Description">Select or edit the prompt template for translation</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_BigModel_Prompt_Placeholder">Select a prompt template</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_BigModel_EditPrompt_ToolTip">Edit Prompt</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_BigModel_EditPrompt_Content">Edit</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_BigModel_AutoTransBack_Header">Back Translation</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_BigModel_AutoTransBack_Description">Automatically perform back translation after translation</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_BigModel_On">On</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_BigModel_Off">Off</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_BigModel_Validate">Validate</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_BigModel_ValidateBtnContent">Validate</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_BigModel_Validate_Description">Test if the API connection and configuration are correct</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_BigModel_Temperature">Temperature Control</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_BigModel_Temperature_Description">Controls the randomness of the model output</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_BigModel_Thinking">Thinking Mode</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_BigModel_Thinking_Description">Controls whether the model performs deeper thinking and reasoning. Supported by GLM-4.5 and above</sys:String>

</ResourceDictionary>